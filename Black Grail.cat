        <profile name="Matagot Hag" typeId="b3af-94f9-2e35-d1c5" typeName="Unit" hidden="false" id="9f6e-ddb1-4c46-1d5b">
          <characteristics>
            <characteristic name="Movement" typeId="b725-e0a4-16ae-8df2">6&quot;/Infantry</characteristic>
            <characteristic name="Ranged" typeId="1a4f-d2cd-c405-6200">+1 Dice</characteristic>
