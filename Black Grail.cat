              </modifiers>
            </selectionEntry>
            <selectionEntry type="upgrade" import="true" name="Enslaved Mind" hidden="false" id="4ea3-6096-0664-a4e6" sortIndex="4">
              <costs>
                <cost name="Ducats" typeId="8d43-33fd-332d-17a6" value="10"/>
                <cost name="Glory Points" typeId="f3bb-a7e6-d476-f60b" value="0"/>
