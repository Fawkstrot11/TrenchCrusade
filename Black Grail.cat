            <selectionEntry type="upgrade" import="true" name=" Startling Speed" hidden="false" id="7e4b-d6b3-582f-5334" sortIndex="11">
              <profiles>
                <profile name="Startling Speed" typeId="7079-589c-df69-fa7e" typeName="Ability" hidden="false" id="d620-b97c-0e00-f1af">
                  <characteristics>
                    <characteristic name="Description" typeId="4834-43a9-1c93-9062">The Homunculus adds +1 DICE to its Dash ACTIONS.</characteristic>
                  </characteristics>
