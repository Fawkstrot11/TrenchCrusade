                    <condition type="atLeast" value="1" field="selections" scope="parent" childId="63e1-fd9c-3c8e-37bf" shared="true"/>
                  </conditions>
                </modifier>
              </modifiers>
            </selectionEntry>
            <selectionEntry type="upgrade" import="true" name="Enslaved Mind" hidden="false" id="1d5d-442e-78c9-6bd2" sortIndex="4">
              <costs>
                <cost name="Ducats" typeId="8d43-33fd-332d-17a6" value="10"/>
                <cost name="Glory Points" typeId="f3bb-a7e6-d476-f60b" value="0"/>
