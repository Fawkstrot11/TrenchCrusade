            <selectionEntry type="upgrade" import="true" name="Inhuman Strength" hidden="false" id="01e9-4879-e909-a64b" sortIndex="6">
              <costs>
                <cost name="Ducats" typeId="8d43-33fd-332d-17a6" value="15"/>
                <cost name="Glory Points" typeId="f3bb-a7e6-d476-f60b" value="0"/>
