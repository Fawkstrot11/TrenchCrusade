      <profiles>
        <profile name="Bite" typeId="090c-b12e-592a-9874" typeName="Weapon" hidden="false" id="a413-643f-2229-c0cc">
          <characteristics>
            <characteristic name="Type" typeId="f90e-171a-4ca6-3845">Special</characteristic>
            <characteristic name="Range" typeId="31a7-b5e8-41dc-5fd1">Melee</characteristic>
            <characteristic name="Modifiers" typeId="6977-37be-e105-b5aa">-</characteristic>
            <characteristic name="Keywords" typeId="8cd6-8018-f2da-5ede">-</characteristic>
            <characteristic name="Rules" typeId="6e95-3480-ad33-b345">Dogs do not suffer penalties for fighting unarmed.</characteristic>
          </characteristics>
        </profile>
        <profile name="Four Paws" typeId="7079-589c-df69-fa7e" typeName="Ability" hidden="false" id="81b5-2b0d-8857-0631">
          <characteristics>
            <characteristic name="Description" typeId="4834-43a9-1c93-9062">Dogs may take any Dash ACTION or jump/Diving Charge ACTION with bonus +1 DICE. They cannot climb sheer surfaces.</characteristic>
          </characteristics>
        </profile>
        <profile name="Trench Dog" typeId="b3af-94f9-2e35-d1c5" typeName="Unit" hidden="false" id="e023-b74f-24ed-98b8">
          <characteristics>
            <characteristic name="Movement" typeId="b725-e0a4-16ae-8df2">8&quot;/Infantry</characteristic>
            <characteristic name="Ranged" typeId="1a4f-d2cd-c405-6200">0</characteristic>
